{
  "name": "buildbranch",
  "description": "Publish a folder to the given build branch (like gh-pages).",
  "version": "1.0.0",
  "homepage": "https://github.com/nfroidure/buildbranch",
  "author": {
    "name": "Nicolas Froidure",
    "url": "http://www.insertafter.com/blog.html"
  },
  "bin": {
    "buildbranch": "./src/bin.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nfroidure/buildbranch.git"
  },
  "scripts": {
    "lint": "eslint src/*.js",
    "preversion": "npm run lint",
    "cli": "env NPM_RUN_CLI=1"
  },
  "bugs": {
    "url": "https://github.com/nfroidure/buildbranch/issues"
  },
  "license": "MIT",
  "main": "src/index",
  "engines": {
    "node": ">= 0.10.0"
  },
  "keywords": [
    "gulpfriendly",
    "gruntfriendly",
    "ghpages",
    "gh-pages",
    "build",
    "publish",
    "git",
    "github"
  ],
  "dependencies": {
    "rimraf": "^2.5.2"
  },
  "devDependencies": {
    "eslint": "^1.10.1",
    "eslint-config-simplifield": "^1.2.2"
  }
}
